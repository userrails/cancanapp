<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>User Info</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name + ' ' + user.email %></td>
        <td><%= user.role.name %></td>
        <td><%= link_to "Show", user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>